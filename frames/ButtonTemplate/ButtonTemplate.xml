<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">



	<Script file="ButtonTemplate.lua"/>

	<Button name="DevsLittleHelperButton" mixin="DevsLittleHelperButtonMixin" virtual="true">
		<Size x="35" y="35"/>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture parentKey="backgroundTexture" atlas="GM-button-marker-available" useAtlasSize="false">
					<Size x="44" y="44"/>
					<Anchors>
						<Anchor point="CENTER" relativePoint="CENTER"/>
					</Anchors>
				</Texture>
			</Layer>
			<Layer level="ARTWORK">
				<Texture parentKey="iconTexture">
					<Size x="26" y="26"/>
					<Anchors>
						<Anchor point="CENTER" relativePoint="CENTER" x="0" y="1"/>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
		<Scripts>
			<OnShow method="OnShow"/>
			<OnEnter method="OnEnter"/>
			<OnLeave method="OnLeave"/>
		</Scripts>
	</Button>

</Ui>